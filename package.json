{
  "name": "muxml",
  "version": "2.0.0",
  "description": "Streaming XML parser and formatter",
  "license": "MIT",
  "repository": "t1st3/muxml",
  "author": {
    "name": "t1st3",
    "email": "contact@t1st3.com",
    "url": "t1st3.com"
  },
  "main": "src/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "xo src/* test/* && mocha",
    "prepublish": "npm test",
    "coverage": "mocha && nyc --reporter=lcov --reporter=text-lcov npm test",
    "precodecov": "npm run coverage",
    "codecov": "cat coverage/lcov.info | ./node_modules/.bin/codecov"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "xml",
    "parse",
    "format",
    "minify",
    "prettify",
    "strip",
    "filter",
    "stream",
    "transform",
    "through"
  ],
  "dependencies": {
    "deep-assign": "^3.0.0",
    "sax": "^1.2.1",
    "through2": "^3.0.0"
  },
  "devDependencies": {
    "codecov": "^3.0.1",
    "concat-stream": "^2.0.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^14.0.0",
    "xo": "^0.24.0"
  },
  "clinton": {
    "rules": {
      "ava": false
    }
  },
  "jsnext:main": "dist/muxml.mjs",
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": true,
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "esnext": true
  }
}
