{
  "name": "muxml",
  "version": "1.0.0",
  "description": "Streaming XML parser and formatter",
  "license": "MIT",
  "repository": "t1st3/muxml",
  "author": {
    "name": "t1st3",
    "email": "contact@t1st3.com",
    "url": "t1st3.com"
  },
  "main": "dist/muxml.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "xo src/* test/* rollup.config.js && npm run build && mocha",
    "prepublish": "npm test",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-lcov npm test",
    "postcoverage": "npm run build",
    "precodecov": "npm run coverage",
    "codecov": "cat coverage/lcov.info | ./node_modules/.bin/codecov"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "xml",
    "parse",
    "format",
    "minify",
    "prettify",
    "strip",
    "filter",
    "stream",
    "transform",
    "through"
  ],
  "dependencies": {
    "deep-assign": "^3.0.0",
    "sax": "^1.2.1",
    "through2": "^3.0.0"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "babelrc-rollup": "^3.0.0",
    "codecov": "^3.0.1",
    "concat-stream": "^2.0.0",
    "cross-env": "^5.1.4",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^13.2.0",
    "rollup": "1.2.3",
    "rollup-plugin-babel": "3.0.7",
    "xo": "^0.23.0"
  },
  "clinton": {
    "rules": {
      "ava": false
    }
  },
  "jsnext:main": "dist/muxml.mjs",
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": true,
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "esnext": true
  }
}
